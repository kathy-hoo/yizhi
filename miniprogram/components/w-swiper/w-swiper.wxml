<!--pages/index/childCpns/w-swiper/w-swiper.wxml-->
<swiper class='swiper' circular autoplay interval="3000" duration="300" indicator-dots indicator-active-color="#8bbd5e">
  <block wx:for='{{list}}' wx:key='index'>
    <swiper-item class='swiper-item'>
      <image mode="scaleToFill" src="{{item.purl}}"></image>
    </swiper-item>
  </block>
</swiper>

